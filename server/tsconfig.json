{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": ["es6"],
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "baseUrl": "./src",
      "outDir": "./dist",
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }
